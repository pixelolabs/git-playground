.mbb-half-heading-half-list-two{ 
    width: 100%;
    background: #f9f4ed;
    padding: 160px 0;
  .container-1200{
    display: flex;
    justify-content: space-between;
    align-items: center;
    .heading{
      width: 89px;
      line-height: 0.94;
      h2{
        position: relative;
        &:before{
          content: " ";
          width: 82px;
          top: -30px;
          left: 0%;
          position: absolute;
          background-image: url({{ "leaf-florish.svg" |asset_url}});
          background-repeat: no-repeat;
          background-size: 100%;
          height: 17px;
      }
        
      }
    }
    ul{
      width: 100%;
      margin-left: 90px;
          li{
            list-style: none;
            width: 25%;
          }
    }
  }
 }