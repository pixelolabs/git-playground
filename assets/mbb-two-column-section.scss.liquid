.mbb-two-column-section{
  width:100%;
  background: #f9f4ed;
  position: relative;
  padding: 70px 0;
  background: #669999;
  .container-1372{
    max-width: 1372px;
    width: 95%;
/*     padding: 0 8px; */
    margin: 0 auto;
    .heading-content{
      text-align: center;
      margin-bottom: 70px;
      position: relative;
      ::before{
        content: " ";
        width: 119px;
        top: -25px;
        left: 46%;
        position: absolute;
        background-image: url({{ "leaf-florish.svg" |asset_url}});
        background-repeat: no-repeat;
        background-size: 100%;
        height:45px
}
          h2{
        font-size: 70px;
        font-family: 'Amatic SC', cursive;
        font-weight: 600;
        color: #fff;
    }
          
    }
    .two-swiper-container{
    margin-left: auto;
    margin-right: auto;
    position: relative;
    overflow: hidden;
    z-index: 1;
    .image-content{
      position: relative;
      max-width: 561px;
      width: 100%;
      left: 0;
      bottom: 0;
      overflow: hidden;
      border-radius: 10px;
      max-height: 450px;
      img{
       object-fit: cover;
       width: 100%;
      }
    }
    }
  }
}


