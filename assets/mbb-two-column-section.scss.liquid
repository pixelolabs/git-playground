.mbb-two-column-section{
  background: #669999;
  .wave-img{
  width:100%;
  top: -12px;
  position: relative;
 }
  .container{
    width: 1440px;
    margin: 0 auto;
    padding: 50px 0;
  
  .heading-content{
      text-align: center;
      margin-bottom: 70px;
    a{
        background-color: #fff;
        border: none;
        color: #231f20;
        padding: 10px 32px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        font-size: 14px;
        border-radius: 25px;
        font-family: 'Montserrat', sans-serif;
        font-weight: 600;
        margin-top: 20px;
      }
    h2{
        font-size: 70px;
        font-family: 'Amatic SC', cursive;
        font-weight: 600;
        color: #fff;
    }
  }
    .carousel{
      ul{
        display: flex;
        width: 100%;
        li{
          width:50%;
          padding: 15px;
          list-style: none;
          img{
            object-fit: cover;
            width: 100%;
            margin-bottom: 20px;
          }
          a{
            font-size: 14px;
            font-weight: 600;
            font-family: Montserrat,sans-serif;
            color: #fff;
            text-decoration: none;
            padding: 10px 0;
            border-bottom: 1px solid #fff;
          }
        }
      }
    }
  }
}