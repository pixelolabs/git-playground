 *{
    margin:0;
    padding:0;
  }
 
  .container-1200{
    max-width: 1200px;
    margin: 0 auto;
    width: 100%;
    padding: 0 10px;
    h2{
      font-size: 70px;
      font-family: 'Amatic SC', cursive;
      font-weight: 600;
    }
  }

.flex-wrap{
  display: flex;
  flex-wrap: wrap;
  li{
     list-style: none;
    h3{
      font-family: 'Montserrat', sans-serif;
    }
    p{
      font-family: 'Montserrat', sans-serif;
    }
    .border-cta {
      font-size: 14px;
      font-weight: 700;
      font-family: Montserrat,sans-serif;
      color: #231f20;
      text-decoration: none;
      padding: 10px 0;
      border-bottom: 2px solid #306767;
      background: none;
      border-radius: 0;
}
  }
}