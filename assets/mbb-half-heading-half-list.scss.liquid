.half-heading-half-list{
  width:100%;
  background: #f9f4ed;
  position: relative;
  padding: 70px 0;
  .container-1182{
    max-width: 1182px;
    width: 100%;
    margin: 0 auto;
    padding: 8px 0;
    display: flex;
    align-items: center;
    .heading{
      flex: 1;
      text-align:right;
      position: relative;
      line-height: 66px;
      h2{
        font-size: 70px;
        font-family: 'Amatic SC', cursive;
        font-weight: 600;
        margin-right: 69px;
        
      }
    }
    ul{
      flex: 7;
      line-height: 66px;
      display: flex;
      flex-wrap: wrap;
      li{
        list-style: none;
        width: 100%;
        max-width: 230px;
        height: 247px;
        background: #fff;
        margin-left: 25px;
        text-align: center;
        border-radius: 10px;
        display: flex;
        flex-direction: column;
        justify-content: center;
        margin-top: 25px;
        .img-dot{
          height: 70px;
          width: 70px;
          border-radius: 50%;
          background: #306767;
          margin: 0 auto;
        }
        h3{
            color: #231f20;
            font-size: 16px;
            font-family: Montserrat,sans-serif;
            margin-bottom: 10px;
            margin-top: 30px;
            line-height: 16px;
          }
          p{
            color: #231f20;
            font-size: 12px;
            font-family: Montserrat,sans-serif;
            margin-bottom: 20px;
            line-height: 16px;
          }
        .border-cta{
          width: 86px;
          margin: 0 auto;
          line-height: 16px;
          padding-top: 0;
        }
      }
    }
  }
}
.half-heading-half-list::before{
      content: " ";
      width:100%;
      top: -12px;
      position: absolute;
      background-image: url({{ "path-2.svg" |asset_url}});
      background-repeat: no-repeat;
      background-size: 100%;
      height:100px;
}
    .half-heading-half-list .container-1182 .heading h2::before{
    content: " ";
    width: 82px;
    top: -15px;
    left: 0%;
    position: absolute;
    background-image: url({{ "leaf-florish.svg" |asset_url}});
    background-repeat: no-repeat;
    background-size: 100%;
    height: 17px;
}
