.four-product-carousel-section{
  width:100%;
  background: #f9f4ed;
  .container-1372{
    max-width: 1372px;
    width:100%;
    padding: 8px 0;
    margin: 0 auto;
    .content{
      width: 25%;
      max-width: 269px;
      .image-section{
        position: relative;
        width: 100%;
        left: 0;
        bottom: 0;
        overflow: hidden;
        border-radius: 10px;
        height: 269px;
        margin-bottom: 20px;
        img{
          width:100%;
          object-fit: cover;
          position: absolute;
          height: 100%;
          left: 0;
          bottom: 0;
        }
        h3{
            font-size: 16px;
            color: #231f20;
            font-weight: 600;
            font-family: 'Montserrat', sans-serif;
            margin-bottom: 10px;
        }
      }
    }
  }
}
